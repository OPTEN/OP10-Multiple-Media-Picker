	<file src="bin\OP10.MultipleMediaPicker.dll" target="lib\net45" />
	<file src="App_Plugins\OP10.MultipleMediaPicker\**\*.*" target="content\App_Plugins\OP10.MultipleMediaPicker" />
  </files>
</package>